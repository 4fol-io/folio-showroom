{
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "border": {
            "radius": true,
            "color": true,
            "style": true,
            "width": true
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "blockGap": null,
            "units": [ "px", "em", "rem", "vh", "vw" ]
        },
        "color": {
			"defaultPalette": true,
            "palette": [
                {
                    "slug": "primary",
                    "color": "#333333",
                    "name": "Primary"
                },
                {
                    "slug": "secondary",
                    "color": "#777777",
                    "name": "Secondary"
                },
                {
                    "slug": "info",
                    "color": "#73EDFF",
                    "name": "Info"
                },
                {
                    "slug": "success",
                    "color": "#38ff90",
                    "name": "Success"
                },
                {
                    "slug": "warning",
                    "color": "#FFE000",
                    "name": "Warning"
                },
                {
                    "slug": "danger",
                    "color": "#FF7D87",
                    "name": "Danger"
                }
            ]
		},
        "layout": {
			"contentSize": "900px",
			"wideSize": "1320px"
		},
        "typography": {
            "customFontSize": true,
            "lineHeight": true,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "textDecoration": true,
            "textTransform": true
        },
        "blocks": {
            "core/group":{
                "spacing": {
                    "blockGap": true
                }
            },
            "core/button":{
                "spacing": {
                    "blockGap": true
                }
            }
        }
    }
}